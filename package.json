{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "dev:app": "remix dev",
    "dev:css": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --watch",
    "dev": "pm2-dev config/pm2.config.js",
    "build:app": "remix build",
    "build:css": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --minify",
    "build": "npm run build:css && npm run build:app",
    "postinstall": "remix setup node",
    "start": "remix-serve api/_build"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "node --require esbuild-register db/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@octokit/core": "^3.5.1",
    "@prisma/client": "^3.6.0",
    "@react-aria/overlays": "^3.7.3",
    "@react-aria/ssr": "^3.1.0",
    "@react-aria/utils": "^3.11.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "airtable": "^0.11.1",
    "clsx": "^1.1.1",
    "collected-notes": "^2.3.0",
    "dotenv": "^10.0.0",
    "gray-matter": "^4.0.3",
    "i18next": "^21.6.2",
    "inflected": "^2.1.0",
    "marked": "^4.0.7",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.1",
    "react-popper": "^2.2.5",
    "remix": "^1.0.6",
    "remix-auth": "^2.5.1",
    "remix-i18next": "^2.1.4",
    "remix-utils": "^2.1.3",
    "tailwindcss": "^3.0.7",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/dotenv": "^8.2.0",
    "@types/inflected": "^1.1.29",
    "@types/marked": "^4.0.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "esbuild-register": "^3.2.1",
    "pm2": "^5.1.2",
    "prisma": "^3.6.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "sideEffects": false
}
