{
	"private": true,
	"name": "sergiodxa.com",
	"description": "",
	"license": "",
	"scripts": {
		"generateCss": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js",
		"predev": "npm run build",
		"dev:app": "remix watch",
		"dev:css": "npm run generateCss -- --watch",
		"dev:http": "cross-env NODE_ENV=development wrangler pages dev ./public --kv airtable",
		"dev": "pm2-dev config/pm2.config.js",
		"build:app": "remix build",
		"build:css": "npm run generateCss -- --minify",
		"build": "npm run build:css && npm run build:app",
		"start": "cross-env NODE_ENV=production npm run dev:wrangler --kv airtable",
		"typecheck": "tsc --noEmit --project tsconfig.json",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"lint:fix": "npm run lint -- --fix",
		"test": "vitest",
		"script": "ts-node --require tsconfig-paths/register",
		"script:download": "npm run script -- scripts/download.ts",
		"update:remix": "npm i @remix-run/cloudflare@latest @remix-run/cloudflare-pages@latest @remix-run/react@latest @remix-run/dev@latest @remix-run/eslint-config@latest"
	},
	"eslintIgnore": [
		"/node_modules",
		"/server/build",
		"/public/build"
	],
	"dependencies": {
		"@markdoc/markdoc": "^0.1.4",
		"@octokit/core": "^3.6.0",
		"@remix-run/cloudflare": "^1.6.8",
		"@remix-run/cloudflare-pages": "^1.6.8",
		"@remix-run/react": "^1.6.8",
		"@tailwindcss/forms": "^0.5.0",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"clsx": "^1.1.1",
		"collected-notes": "^2.3.0",
		"dotenv": "^16.0.0",
		"i18next": "^21.6.16",
		"i18next-browser-languagedetector": "^6.1.4",
		"i18next-fetch-backend": "^4.1.1",
		"nprogress": "^0.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^11.16.7",
		"remix-auth": "^3.1.0",
		"remix-auth-email-link": "^1.3.1",
		"remix-auth-form": "^1.1.1",
		"remix-auth-github": "^1.1.0",
		"remix-i18next": "^4.0.0",
		"remix-utils": "^3.0.0",
		"tiny-invariant": "^1.2.0",
		"zod": "^3.14.4"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.14.1",
		"@faker-js/faker": "^6.1.2",
		"@remix-run/dev": "^1.6.8",
		"@remix-run/eslint-config": "^1.6.8",
		"@remix-run/node": "^1.6.8",
		"@testing-library/jest-dom": "^5.16.4",
		"@types/dotenv": "^8.2.0",
		"@types/inflected": "^1.1.29",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"@vitejs/plugin-react": "^1.3.1",
		"c8": "^7.11.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prefer-let": "^3.0.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"execa": "^6.1.0",
		"get-port": "^6.1.2",
		"happy-dom": "^3.2.0",
		"pm2": "^5.2.0",
		"pptr-testing-library": "^0.7.0",
		"prettier": "^2.6.2",
		"prettier-plugin-tailwindcss": "^0.1.10",
		"puppeteer": "^13.6.0",
		"tailwindcss": "^3.0.24",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.6.3",
		"vite-tsconfig-paths": "^3.4.1",
		"vitest": "^0.9.4",
		"wrangler": "^2.0.26"
	},
	"sideEffects": false
